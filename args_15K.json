{
	"datasetPath": "datasets/",
  	"dataset_division": "721_5fold/",
	"output": "output/",

	"ordered" : true,
  	"train_epochs": 1001,
	"neg_k": 50,
	"metric": "L1",
	"cuda": false,
	"seed": 72,
	"early_stop": true,
	"start_valid": 20,
	"eval_freq": 10,
	"eval_save_freq": 20,
	"sample_neg_freq": 20,
    "optim_type": "Adagrad",
	"patience": 20,
	"patience_val": 20,

	// Super Parameter
	"learning_rate": 0.01,
	"weight_decay": 5e-4,
	"LeakyReLU_alpha": 0.2,
	"dropout": 0,
	"alpha1": 1.0,
	"alpha2": 1.0,
	"gamma": 5.0,

	"alpha3": 1.0,
	"l_bata": 0.5,

	"top_k": [1, 3, 5, 10, 50, 100],
	"csls_k": 5
}

